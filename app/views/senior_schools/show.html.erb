<div class="d-grid gap-2 d-md-flex justify-content-md-end">
  <%= link_to 'BACK',senior_schools_path, class: 'btn btn-secondary'  %>
</div><br>
<div class="card">
	<div class="card-header header-style">
		<h4><%= @senior_school_name.school_name %> BLUE PRINT UPLOADED DATA</h4>
	</div>
  <% unless @senior_school.nil? || @senior_school.empty? %>
	<div class = "card-body">
		<div><h3 class="tool_bar"></h3></div>
		<div class="table-responsive">
			<table id="student_table" class="table table-bordered">
				<thead>
					<tr>
						<th>Student Id</th>
						<th>First Name</th>
						<th>Last Name</th>
						<th>Online Code</th>
						<th>Email</th>
					</tr>
				</thead>
				<tbody>
					<% @senior_school.each do |stu| %>
						<tr>
							<td><%= stu.internal_student_id%></td>
							<td><%= stu.first_name %></td>
							<td><%= stu.last_name %></td>
							<td><%= stu.online_code %></td>
							<td><%= stu.email %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
	<% else %>
		<div class = "card-body">
			<p>Currently there is no data to show.</p>
		</div>
	<% end %>
	</div>
	</div>
</div>


